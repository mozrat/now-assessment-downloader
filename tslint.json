{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-no-unused-expression-chai"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [true, "single", "avoid-escape", "avoid-template"],
        "no-var-requires": false,
        "no-console": false,
        "trailing-comma": false,
        "no-shadowed-variable": false
    }
}